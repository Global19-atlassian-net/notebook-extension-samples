{
  "name": "notebook-editing",
  "displayName": "Notebook Editing",
  "description": "Notebook Editing",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "version": "1.0.0",
  "publisher": "vscode",
  "icon": "icon.png",
  "enableProposedApi": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.40.0"
  },
  "main": "./out/extension",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:notebookEditing"
  ],
  "contributes": {
    "notebookProvider": [
      {
        "viewType": "notebookEditing",
        "displayName": "Notebook Editing",
        "selector": [
          {
            "filenamePattern": "*.notebook"
          }
        ]
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "npm run build-lib && gulp watch-extension:notebook-editing",
    "build": "webpack-cli --config extension.webpack.config.js",
    "build-lib": "webpack --mode production"
  },
  "dependencies": {
    "typescript": "^3.8.2",
    "vscode-extension-telemetry": "0.1.1",
    "vscode-nls": "^4.0.0"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.10",
    "@types/node": "^6.0.40"
  }
}
